{
    "name": "express-typescript-starter-template",
    "version": "1.0.0",
    "description": "Express.js Starter Template based on Typescript",
    "main": "src/index.ts",
    "scripts": {
        "dev": "nodemon src/index.ts",
        "build": "rimraf dist && tsc",
        "serve": "node dist/index.js",
        "generate": "graphql-codegen --config codegen.yml",
        "format": "prettier --write \"**/*.ts\"",
        "build-and-serve": "npm run build && npm run serve"
    },
    "author": "Developer",
    "license": "ISC",
    "dependencies": {
        "@apollo/server": "^4.7.1",
        "aws-sdk": "^2.1370.0",
        "axios": "^1.4.0",
        "bcrypt": "^5.1.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-validator": "^7.0.1",
        "graphql": "^16.6.0",
        "graphql-import-files": "^1.2.19",
        "helmet": "^6.1.5",
        "jsonwebtoken": "^9.0.0",
        "mongoose": "^7.1.0",
        "morgan": "^1.10.0",
        "nanoid": "^4.0.2",
        "pino": "^8.14.1",
        "rate-limiter-flexible": "^2.4.1",
        "uuid": "^9.0.0",
        "winston": "^3.8.2"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "^4.0.1",
        "@graphql-codegen/typescript": "^4.0.0",
        "@graphql-codegen/typescript-resolvers": "^4.0.0",
        "@types/bcrypt": "^5.0.0",
        "@types/compression": "^1.7.2",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/jsonwebtoken": "^9.0.2",
        "@types/morgan": "^1.9.4",
        "@types/node": "^18.16.3",
        "@types/request-ip": "0.0.37",
        "@types/uuid": "^9.0.2",
        "@typescript-eslint/eslint-plugin": "^5.59.2",
        "@typescript-eslint/parser": "^5.59.2",
        "eslint": "^8.39.0",
        "nodemon": "^2.0.22",
        "prettier": "^2.8.8",
        "rimraf": "^5.0.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.4"
    }
}
